<%- include("../_header.ejs") -%>

<% model.Titre = "Ajouter un tableau" %>

<h2><%= model.Titre %></h2>

<form action="/tableaux/create" method="post">
  <div class="form-horizontal">

    <%- include("_editor.ejs", {model: model}) -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Ajouter" class="btn btn-default btn-success" />
        <a href="/" class="btn btn-outline-dark">Annuler</a>
      </div>
    </div>

  </div>
</form>

<%- include("../_footer.ejs") -%>

<%- include("../_form-scripts.ejs") -%>
