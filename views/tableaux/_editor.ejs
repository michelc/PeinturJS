<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Nom">Nom</label>
  <div class="col-sm-10">
    <input autofocus="" class="form-control" id="Nom" name="Nom" type="text" value="<%= model.Nom %>" />
  </div>
</div>

